<%- include('header'); -%>

<div class="box" id="heading">
    <h1><%=kindOfDay%></h1>
</div>
<div class="box">
    <% for (let index = 0; index < listItem.length; index++) { %>
    <div class="item">
        <input type="checkbox">
        <p><%= listItem[index] %></p>
    </div>
    <% } %>
    <form class="item" action="/" method="post">
        <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
        <button type="submit">+</button>
    </form>
</div>

<%- include('footer'); -%>